package radialMenu.mapmenu;

/**
 *
 * @author jmoura
 */
public enum MapMenuRadialActions {

    //MENU BUILD
    LOCAL_INFO("MENU.LOCAL.INFO"),
    RANGE_PLOT("MENU.LOCAL.RANGE"),
    COMBAT_SIMULATOR("MENU.COMBAT.SIMULATOR");
    //internal fields
    private final String label;

    MapMenuRadialActions(String label) {
        this.label = label;
    }

    public String getLabel() {
        return this.label;
    }
}
